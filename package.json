{
    "name": "webServiceExtension",
    "displayName": "Web Service Extension",
    "description": "Extension to interact with a web service",
    "version": "1.0.0",
    "engines": {
        "vscode": "^1.58.0"
    },
    "main": "extension.js",
    "activationEvents": [
        "onCommand:extension.activateWebService"
    ],
    "contributes": {
        "commands": [
            {
                "command": "extension.activateWebService",
                "title": "Activate Web Service"
            }
        ]
    },
    "scripts": {
        "postinstall": "node ./node_modules/vscode/bin/install"
    },
    "dependencies": {
        "vscode": "^1.58.0"
    }
}